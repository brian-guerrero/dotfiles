{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\ue285 ",
    "foreground": "#f2f4f8",
    "background": "transparent"
  },
  "terminal_background": "#161616",
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_host": true
          },
          "leading_diamond": "<#ee5396> \ue200 </>",
          "template": "{{ .UserName }} <#f2f4f8>on</>",
          "foreground": "#78a9ff",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "Monday <#f2f4f8>at</> 3:04 PM"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#be95ff",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": " \ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#ee5396",
          "type": "git",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "foreground": "#19f9d8",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "dallas",
            "threshold": 0
          },
          "template": " {{ .FormattedMs }}s <#f2f4f8>\ue601</>",
          "foreground": "#3ddbd9",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf292 "
          },
          "template": " \uf0e7 ",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " <#f2f4f8>MEM:</> {{ round .PhysicalPercentUsed .Precision }}% ({{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB)",
          "foreground": "#25be6a",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\ueaf7",
            "folder_separator_icon": " \uf054 ",
            "home_icon": "home",
            "style": "agnoster_full"
          },
          "leading_diamond": "<#33b1ff> \ue285 </><#ff7eb6>{</>",
          "trailing_diamond": "<#ff7eb6>}</>",
          "template": " \ue5ff {{ .Path }} ",
          "foreground": "#ff7eb6",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}\udb80\udd59{{else}}\uebb3{{ end }}  ",
          "foreground": "#3ddbd9",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ee5396{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}