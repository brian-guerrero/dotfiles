{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\ue285 ",
    "foreground": "#FEF5ED",
    "background": "transparent"
  },
  "terminal_background": "#242526",
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_host": true
          },
          "leading_diamond": "<#ff70a6> \ue200 </>",
          "template": "{{ .UserName }} <#ffffff>on</>",
          "foreground": "#6FC1FF",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "Monday <#ffffff>at</> 3:04 PM"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#B084EB",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": " \ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#FF2C6D",
          "type": "git",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "foreground": "#19f9d8",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "dallas",
            "threshold": 0
          },
          "template": " {{ .FormattedMs }}s <#ffffff>\ue601</>",
          "foreground": "#19f9d8",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf292 "
          },
          "template": " \uf0e7 ",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " <#ffffff>MEM:</> {{ round .PhysicalPercentUsed .Precision }}% ({{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB)",
          "foreground": "#6FE7D2",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\ueaf7",
            "folder_separator_icon": " \uf054 ",
            "home_icon": "home",
            "style": "agnoster_full"
          },
          "leading_diamond": "<#00c7fc> \ue285 </><#FF4B82>{</>",
          "trailing_diamond": "<#FF4B82>}</>",
          "template": " \ue5ff {{ .Path }} ",
          "foreground": "#FF4B82",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}\udb80\udd59{{else}}\uebb3{{ end }}  ",
          "foreground": "#19f9d8",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
